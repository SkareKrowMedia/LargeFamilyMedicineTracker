<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.skarekrowmedia.largefamilymedicinetracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LargeFamilyMedicineTracker">
        <activity
            android:name=".EditChildActivity"
            android:exported="false"
            android:label="@string/title_activity_edit_child"
            android:theme="@style/Theme.LargeFamilyMedicineTracker.NoActionBar" />
        <activity
            android:name=".ViewChildActivity"
            android:exported="false"
            android:label="@string/title_activity_view_child"
            android:theme="@style/Theme.LargeFamilyMedicineTracker.NoActionBar" />
        <activity
            android:name=".AddChildActivity"
            android:exported="false"
            android:label="@string/title_activity_add_child"
            android:theme="@style/Theme.LargeFamilyMedicineTracker.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>